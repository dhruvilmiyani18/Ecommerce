<div class="form-group row">
    <label class="col-sm-12 col-md-2 col-form-label">Select Type</label>
    <div class="col-sm-12 col-md-10">
        <select class="custom-select col-12" name="typeId" id="typeId">
            <option selected="">--Select Type--</option>
            <% for (var td of typedata) { %>
                <option value="<%= td.id %>">
                    <%= td.typename %>
                </option>
                <% } %>
        </select>
    </div>
</div>

<div class="form-group row">
    <label class="col-sm-12 col-md-2 col-form-label">Select Brand</label>
    <div class="col-sm-12 col-md-10">
        <select class="custom-select col-12" name="brandId" id="brandId">
            <option selected="">--Select Brand--</option>
            <% for (var bd of branddata) { %>
                <option value="<%= bd.id %>">
                    <%= bd.brandname %>
                </option>
                <% } %>
        </select>
    </div>
</div>