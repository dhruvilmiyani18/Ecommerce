<%- include('../header') %>

<div class="main-container">
    <div class="pd-20 card-box mb-30">

        <form action="/subcategory/subcategory_insertdata" method="post" >
            
           
            <div class="form-group row">
                <label class="col-sm-12 col-md-2 col-form-label">Select Category</label>
                <div class="col-sm-12 col-md-10">
                    <select class="custom-select col-12" name="categoryId">
                        <option value="">---Select Category---</option>
                        <% for(var ct of categorydata){ %>
                            <option  value="<%= ct.id %>"><%= ct.category %></option>
                        <% } %>
                    </select>
                </div>
            </div>

            <div class="form-group row">
                <label class="col-sm-12 col-md-2 col-form-label ">Enter Subcategory</label>
                <div class="col-sm-12 col-md-10">
                    <input class="form-control" name="subcategory" type="text" placeholder="Enter SubCAtegory "/>
                </div>
            </div>

            <div class="form-group ">
                <input type="submit" name="submit" id="" class="btn btn-success mt-4">
            </div>
           
        </form>
    </div>
</div>
  

                    <%- include('../footer') %>

                  