<%- include('../header') %>
    <div class="main-container">
        <div class="pd-ltr-20 xs-pd-20-10">
            <div class="min-height-200px">
                <div class="page-header">
                    <div class="row">
                        <div class="col-md-6 col-sm-12">
                            <div class="title">
                                <h4>Category Form</h4>
                            </div>

                        </div>
                        <div class="col-md-6 col-sm-12 text-right">
                            <div class="dropdown">
                                <a class="btn btn-secondary " href="#" role="button"
                                    data-toggle="dropdown"> Date :
                                 <%= date %>
                                </a>
                               
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Default Basic Forms Start -->

                <div class="pd-20 card-box mb-30">

                    <form action="/category/category_insrtData" method="post" enctype="multipart/form-data">
                        <div class="form-group row">
                            <label class="col-sm-12 col-md-2 col-form-label">Enter Category :</label>
                            <div class="col-sm-12 col-md-10">
                                <input class="form-control" type="text" name="category" placeholder="Johnny Brown" />
                            </div>
                        </div>

                       

                        <div class="form-group row">
                            <label class="col-sm-12 col-md-2 col-form-label">Upload Image :</label>
                            <div class="custom-file col-sm-12 col-md-10">
                                <input type="file" name="categoryImages" class="custom-file-input form-control" />
                                <label class="custom-file-label">Choose file</label>
                            </div>
                        </div>

                   
                        <div class="form-group ">
                            <input type="submit" name="submit" id="" class="btn btn-success mt-4">
                        </div>

                    </form>

                    <%- include('../footer') %>