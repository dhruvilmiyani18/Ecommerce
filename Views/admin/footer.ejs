          layout : 'topCenter',
          theme: 'metroui',
        }).show();
    <% } %>
    
    
    <% if(flash.error && flash.error.length > 0) { %>
        new Noty({
          type: 'error',
          text: '<%= flash.error %>',
          layout : 'topCenter',
          theme: 'metroui',  
        }).show();
    <% } %>
    
    </script>
