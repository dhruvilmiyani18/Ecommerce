<script>

    function getSubcategoryData(){
  
        var catId = document.getElementById('categoryId').value;

        $.ajax({
            url : "/extracategory/getsubcategory",
            method : "post",
            data :{
                "categoryId" : catId
            },
            success :(res)=>{
                $('#subcategoryId').html(res);
              
            }
        })
    }

    function getExtraCategoryData(){
        var subCatId = document.getElementById('subcategoryId').value;

        $.ajax({
            url :"/brand/getExtraCategory",
            method :"post",
            data :{
                "subcategoryId" : subCatId
            },
            success :(res)=>{
                $('#extracategoryId').html(res);
            }
        })
    }

     function getSubCategoryData(){
        var catId = document.getElementById('categoryId').value;
        $.ajax({
            url :'/type/getsubcategorydata',
            method : "post",
            data : {
                'categoryId' : catId
            },
            success :(res)=>{                
                $('#subcategoryId').html(res);
            }
        })
        
    }

    function getTypeAndBrandData(){
        var extraCatId = document.getElementById('extracategoryId').value;
        $.ajax({
            url :'/product/getTypeAndBrandData',
            method :'post',
            data :{
                'extracategoryId' : extraCatId
            },
            success :(res)=>{
                $('#brandtypechenge').html(res);
            }
        })
    }
  

</script>
