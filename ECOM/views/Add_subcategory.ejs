    <!-- ============================================================== -->
    <!-- end pageheader -->
    <!-- ============================================================== -->
 
       
        <div class="row">
            <!-- ============================================================== -->
            <!-- basic form -->
            <!-- ============================================================== -->
            <div class="col-xl-6 col-lg-6 col-md-12 col-sm-12 col-12">
                <div class="card">
                    <h5 style="color:brown; font-size: 35px;" class="card-header" >Basic Form</h5>
                    <% if(flash.success && flash.success.length > 0) { %>
                        <div class="alert alert-success">
                            <%= flash.success %>
                          </div>
                    <% } %>

                    <% if(flash.error && flash.error.length > 0) { %>
                        <div class="alert alert-danger">
                            <%= flash.error %>
                          </div>
                    <% } %>
                    <div class="card-body">
                        <form action="/subcategory/insertCategory" id="basicform"  method="post">

                            <div class="form-group">
                                <label for="inputUserName">Enter Category</label>
                                <select class="form-control">
                                        <option value="">--select Category--</option>
                                        <% for(var cd of catdata){%>
                                        <option value="<%= cd.id %>"><%= cd.category_name %></option>
                                        <% } %>
                                </select>
                            </div>


                            <div class="form-group">
                                <label for="inputUserName">Enter Category</label>
                                <input id="inputUserName" type="text" name="category_name" data-parsley-trigger="change" required="" placeholder="Enter category Name" autocomplete="off" class="form-control">
                            </div>

                            
                            
                            <div class="row">
                               
                                <div class="col-sm-6 pl-0">
                                    <p class="text-right">
                                        <input  type="submit" class="btn btn-space btn-primary" value="Add Admin" name="Submit">
                                        <button class="btn btn-space btn-secondary">Cancel</button>
                                    </p>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
     
        </div>
       

</div>
<!-- ============================================================== -->
<!-- footer -->
<!-- ============================================================== -->
